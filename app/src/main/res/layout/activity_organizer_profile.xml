<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.codefordenver.encorelink.MainScreenActivity">


    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:text="Profile"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/organizationNameView"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_row="1"
        android:layout_column="0"
        android:text="Organization name*: " />

    <EditText
        android:id="@+id/organizationName"
        android:layout_width="231dp"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:layout_row="1"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/streetAddressView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="0"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="2dp"
        android:layout_row="2"
        android:text="Street Address: "

        android:layout_marginStart="10dp" />

    <EditText
        android:id="@+id/streetAddress"

        android:layout_width="231dp"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"


        android:layout_row="2"
        android:layout_column="1"
         />

    <TextView
        android:id="@+id/cityView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="2dp"
        android:layout_column="0"
        android:layout_row="3"
        android:text="City:" />

    <TextView
        android:id="@+id/zipcodeView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_column="0"
        android:layout_row="5"
        android:text="Zipcode:" />

    <EditText
        android:id="@+id/city"
        android:layout_width="231dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:textSize="14sp"
        android:layout_row="3"
        android:layout_column="1"
         />

    <EditText
        android:id="@+id/zipcode"
        android:layout_width="231dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:textSize="14sp"
        android:layout_column="1"
        android:layout_row="5" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_column="0"
        android:layout_row="6"
        android:text="About You"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/contactNameView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_column="0"
        android:layout_row="7"
        android:text="Contact name*:" />

    <EditText
        android:id="@+id/contactName"
        android:layout_width="231dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:textSize="14sp"
        android:layout_column="1"
        android:layout_row="7" />

    <TextView
        android:id="@+id/contactJobTitleView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_column="0"
        android:layout_row="8"
        android:text="Contact job title: " />

    <EditText
        android:id="@+id/contactJobTitle"
        android:layout_width="231dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:textSize="14sp"
        android:layout_column="1"
        android:layout_row="8" />

    <TextView
        android:id="@+id/phoneNumberView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_column="0"
        android:layout_row="9"
        android:text="Phone Number*:" />

    <TextView
        android:id="@+id/emailAddressView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_column="0"
        android:layout_row="10"
        android:text="Email Address: " />

    <EditText
        android:id="@+id/phoneNumber"
        android:layout_width="231dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:textSize="14sp"
        android:layout_column="1"
        android:layout_row="9" />

    <EditText
        android:id="@+id/emailAddress"
        android:layout_width="231dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:textSize="14sp"
        android:layout_column="1"
        android:layout_row="10" />

    <Button
        android:id="@+id/pushToDB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_marginBottom="20dp"
        android:layout_row="11"
        android:text="Save" />

    <TextView
        android:id="@+id/stateView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_column="0"
        android:layout_row="4"
        android:text="State: " />

    <EditText
        android:id="@+id/state"
        android:layout_width="231dp"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:layout_row="4"
        android:textSize="14sp" />

</GridLayout>
